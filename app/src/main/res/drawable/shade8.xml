<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners
                android:bottomRightRadius="100dp"
                />
            <gradient
                android:angle="-45"
                android:endColor="@color/color72"
                android:startColor="@color/color71" />
        </shape>
    </item>



    <item>
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="100dp"
                />
            <gradient
                android:angle="-45"
                android:endColor="@color/color92"
                android:startColor="@color/color91" />
        </shape>
    </item>



    <item>
        <shape android:shape="rectangle">
            <corners
                android:bottomRightRadius="100dp"
                android:topLeftRadius="100dp" />
            <gradient
                android:angle="-45"
                android:endColor="@color/color81"
                android:startColor="@color/color82" />
        </shape>
    </item>
</layer-list>